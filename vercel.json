{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "vite.config.ts",
        "use": "@vercel/vite"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/index.ts"
      },
      {
        "src": "/(.*)",
        "dest": "vite.config.ts"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "server/index.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }